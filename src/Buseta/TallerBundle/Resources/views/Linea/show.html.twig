{% extends '::base.html.twig' %}

{% block title block('header') %}

{% block header 'Ver Datos de Lineas' %}

{% block body -%}
    <h1>Linea</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Numero</th>
                <td>{{ entity.numero }}</td>
            </tr>
            <tr>
                <th>Tipo</th>
                <td>{{ entity.tipo }}</td>
            </tr>
            <tr>
                <th>Cantidad_pedido</th>
                <td>{{ entity.cantidadpedido }}</td>
            </tr>
            <tr>
                <th>Monto</th>
                <td>{{ entity.monto }}</td>
            </tr>
        </tbody>
    </table>

    <div class="btn-group">
        <a class="btn btn-primary" href="{{ path('ordentrabajo_edit', { 'id': entity.id }) }}">
            <i class="fa fa-edit"></i> Editar</a>
    </div>

    <div class="btn-group">
        <a class="btn btn-default" href="{{ path('ordentrabajo') }}">
            <span class="glyphicon glyphicon-chevron-left"></span> Listado</a>
    </div>


{% endblock %}


{% block contenido %}


{% endblock %}
