<div data-keyboard="false" data-backdrop="static" class="modal" id="form_onecprod_modal" tabindex="-1" role="dialog"
     aria-labelledby="createONecProd" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                <h4 class="modal-title"
                    id="createONecProd">{{ 'orden_necesidad_producto.btn.adicionar'|trans({},'BusetaTallerBundle') }}</h4>
            </div>
            <div class="modal-body">
                <form name="{{ edit_form.vars.full_name }}" id="{{ edit_form.vars.id }}"
                      action="{{ edit_form.vars.action }}"
                      method="post" enctype="multipart/form-data">
                    {% if edit_form.vars.method == 'PUT' %}
                        <input type="hidden" name="_method" value="PUT"/>
                    {% endif %}
                    {{ form_errors(edit_form) }}
                    {{ form_widget(edit_form) }}
                    {{ form_rest(edit_form) }}
                </form>
            </div>
            <div class="modal-footer">
                <a href="#" class="btn btn-default" id="btn_onecprod_cancel" data-dismiss="modal"><span
                            class="glyphicon glyphicon-chevron-left"></span>
                    {{ 'actions.cancel' | trans({}, 'BusetaTallerBundle') }}</a>
                <a href="#" class="btn btn-primary" id="btn_onecprod_save"><span
                            class="glyphicon glyphicon-save"></span>
                    {{ 'actions.save' | trans({}, 'BusetaTallerBundle') }}</a>
            </div>
        </div>
    </div>
</div>
