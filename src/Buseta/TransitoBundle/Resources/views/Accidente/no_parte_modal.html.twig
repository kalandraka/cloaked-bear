<div class="modal" id="form_noparte_modal" role="dialog" aria-labelledby="noparteForm"
     aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button id="closeModal" type="button" class="close" data-dismiss="modal"
                        aria-hidden="true">&times;</button>
                <h4 class="modal-title"
                    id="noparteForm">{{ 'accidente.no_parte'|trans({},'BusetaTransitoBundle') }}</h4>
            </div>

            <div class="modal-body">
                <label class="control-label">Responsable</label>
                <select id="buseta_transitobundle_accidente_responsable" class="form-control">
                    <option value="NOHUBO">No hubo
                    <option value="CHOFER">Chofer
                    <option value="EMPRESA">Empresa
                    <option value="TERCERO">Tercero
                </select>

                <label class="control-label">Quien solventa</label>
                <select id="buseta_transitobundle_accidente_quienPaga" class="form-control">
                    <option value="NADIE">Nadie
                    <option value="CHOFER">Chofer
                    <option value="EMPRESA">Empresa
                    <option value="TALLER">Taller
                    <option value="TERCERO">Tercero
                    <option value="SEGURO">Seguro
                </select>
            </div>

            <div class="modal-footer">
                <a class="btn btn-default" id="btn_noparte_cancel" data-dismiss="modal"><span
                            class="glyphicon glyphicon-chevron-left"></span>
                    {{ 'actions.cancel' | trans({}, 'BusetaTransitoBundle') }}</a>
                <a id="btn_noparte_aceptar" class="btn btn-primary">
                    <span class="glyphicon glyphicon-primary"></span>
                    {{ 'actions.confirm' | trans({}, 'BusetaTransitoBundle') }}
                </a>
            </div>
        </div>
    </div>
</div>
